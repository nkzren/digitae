{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-mixed-spaces-and-tabs": "error",
        "indent": ["error", 2]
    }
}
